{
  "hostname": "MOS",
  "keymap": "de-latin1-nodeadkeys",
  "timezone": "Europe/Vienna",
  "display": {
    "timeout": 30,
    "powersave": "on",
    "powerdown": 60
  },
  "ntp": {
    "enabled": true,
    "mode": "iburst",
    "servers": [
      "0.pool.ntp.org",
      "1.pool.ntp.org"
    ]
  },
  "notification_sound": {
    "startup": true,
    "reboot": true,
    "shutdown": true
  },
  "global_spindown": "20",
  "cpufreq": {
    "governor": "ondemand",
    "max_speed": 0,
    "min_speed": 0
  },
 "swapfile": {
    "enabled": false,
    "path": null,
    "size": null,
    "priority": -2,
    "config": {
      "zswap": false,
      "shrinker": true,
      "max_pool_percent": 20,
      "compressor": "zstd",
      "zpool": "zsmalloc",
      "accept_threshold_percent": 90
    }
  }
}
